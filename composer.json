    "description": "Easily edit Symfony2 translations while browsing your site",
    "keywords": ["translator", "translations", "i18n", "ui", "bundle"],
    "homepage": "http://github.com/domis86/TranslatorBundle",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Dominik Frankowicz",
            "email": "domis86@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": ">=2.6",
        "symfony/framework-bundle": ">=2.3",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0"
    },
    "autoload": {
        "psr-0": { "Domis86\\TranslatorBundle\\": "" }
    },
    "target-dir": "Domis86/TranslatorBundle",
    "minimum-stability": "dev"
